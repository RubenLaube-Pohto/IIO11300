<Window x:Class="Tehtava5.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tehtava5"
        mc:Ignorable="d"
        Title="SnakeWPF" Height="203.258" Width="214.66">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0">
            <Button x:Name="btnStart" Content="New Snake Game" Margin="0,0,0,10"/>
            <Button x:Name="btnHighscores" Content="Highscores" Margin="0,0,0,10"/>
            <Button x:Name="btnQuit" Content="Quit" Click="btnQuit_Click"/>
        </StackPanel>
    </Grid>
</Window>
